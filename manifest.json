{
    "manifest_version": 2,
    "name": "Screenshot Extension",
    "version": "1.0",
    "description": "Capturing screenshots",
    "icons": {  
      "48": "icon48.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {  
        "48": "icon48.png" 
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }